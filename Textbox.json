{
	"name": "TextboxTemplate",
	"visualStyles": {
		"textbox": {
			"*": {
				"*": [{
					"fontSize": 40,
					"fontFamily": "Calibri",
					"wordWrap": true,
					"color": { "solid": { "color": "#FFFFFF"} }
				}],
				"background": [{
					"show": true,
					"color": { "solid": { "color": "#DDDDDD"}},
					"transparency": 0
				}],
				"visualHeader": [{
					"show": true
				}]
			}
		}
	}
}
